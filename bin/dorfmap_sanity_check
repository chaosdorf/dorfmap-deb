ERRBASE=/tmp/dorfmap-err

for warnfile in dorfmap_set_remote; do
	if [ -e "${ERRBASE}/${warning}" ]; then
		warnblink yellow &
		break
	fi
done

for critfile in door_set_private; do
	if [ -e "${ERRBASE}/${critfile}" ]; then
		warnblink red &
		break
	fi
done
